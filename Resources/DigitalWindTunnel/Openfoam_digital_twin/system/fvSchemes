FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default     backward;
}

gradSchemes
{
    default         Gauss linear;
    grad(p)         Gauss linear;
    grad(U)         Gauss linear;
}

divSchemes
{
    default         none;
    div(phi,U)      bounded Gauss linearUpwind Gauss linear;
    div(phi,k)      bounded Gauss linearUpwind Gauss linear;
    div(phi,epsilon) bounded Gauss linearUpwind Gauss linear;
    div(phi,R)      bounded Gauss linearUpwind Gauss linear;
    div(R)          Gauss linear;
    div(phi,nuTilda) bounded Gauss linearUpwind Gauss linear;
    div((nuEff*dev2(T(grad(U))))) Gauss linear;
    div(phi,omega)  bounded Gauss linearUpwind Gauss linear;
    div(DomegaEff,omega)  Gauss linear;
    div(phi,flm)  bounded Gauss linearUpwind Gauss linear;
    div(phi,fmm)  bounded Gauss linearUpwind Gauss linear;
    div(phi,B)  bounded Gauss linearUpwind Gauss linear;
    div(B)  Gauss linear;
}

laplacianSchemes
{
    default         Gauss linear corrected;
    laplacian(nu,U) Gauss linear corrected;
    laplacian(nuEff,U) Gauss linear corrected;
    laplacian((1|A(U)),p) Gauss linear corrected;
    laplacian(DkEff,k) Gauss linear corrected;
    laplacian(DepsilonEff,epsilon) Gauss linear corrected;
    laplacian(DREff,R) Gauss linear corrected;
    laplacian(DnuTildaEff,nuTilda) Gauss linear corrected;
    laplacian(DomegaEff,omega) Gauss linear corrected;
    laplacian(phi,omega) Gauss linear corrected;
}

interpolationSchemes
{
    default         linear;
    interpolate(U)  linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         no;
    p;
}

// ************************************************************************* //
